    "\n",
    "@app.route('/')\n",
    "def home():\n",
    "  return \"hello. i am alive\"\n",
    "\n",
    "\n",
    "def run():\n",
    "  app.run(host='0.0.0.0',port=8080)\n",
    "\n",
    "\n",
    "def keep_alive():\n",
    "  t= Thread(target=run)\n",
    "  t.start()"
